generator client {
	provider = "prisma-client-js"
	previewFeatures = [ "mongoDb" ]
}

datasource db {
	provider = "mongodb"
	url = env("DATABASE_URL")
}

model User {
	id		String	@id @default(dbgenerated()) @map("_id") @db.ObjectId
	email		String	@unique
	username	String	@unique
	password	String
	avatarUrl	String
}
